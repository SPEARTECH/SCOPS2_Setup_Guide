                                text: 'Press TRIANGLE at the main menu to view your MAC Address'
                                # font_style: 'H6'
                                halign: 'center'
                                size_hint_y: .1
                            BoxLayout:
                                orientation: 'horizontal'
                                size_hint_y: .2
                                MDBoxLayout:
                                    orientation:'vertical'
                                    padding: 15
                                    MDRaisedButton:
                                        text: 'Back'
                                        on_release: 
                                            screen_manager.transition.direction = 'right'
                                            app.back_button('console1')#screen_manager.current = 'console'
                                        pos_hint: {"center_x": .5, "center_y": .5}
                                MDBoxLayout:
                                    orientation:'vertical'
                                    padding: 15
                                    MDProgressBar:
                                        value: 20
