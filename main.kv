MDNavigationLayout:
    ip_info: ip_info
    screen_manager: screen_manager
    staticiplabel: staticiplabel
    ScreenManager:
        Screen:
            BoxLayout:
                orientation: 'vertical'
                MDToolbar:
                    halign: 'center'
                    title: 'Splinter Cell Online PS2'
                    type: 'top'
                    elevation: 10
                    anchor_title: 'center'
                    left_action_items: [["menu", lambda x: nav_drawer.set_state()]]
                    right_action_items: [["share-variant", lambda x: app.share()]]
                ScreenManager:
                    id: screen_manager
                    Screen:
                        name: 'screen1'
                        BoxLayout:
                            orientation: 'vertical'
                            BoxLayout:
                                orientation: 'vertical'
                                padding: 0,40,0,0
                                MDLabel:
                                    text: 'Welcome to the community!' 
                                    font_style: 'H3'
                                    halign: 'center'
                                MDLabel:
                                    text: 'Navigate the menus and setup with XLink Kai.' 
                                    font_style: 'Subtitle1'
                                    halign: 'center'
                                    size_hint_y: .3
                                MDLabel:
                                    text: 'Refer to the #setup-help section of Discord for assistance.'
                                    font_style: 'Subtitle2'
                                    halign: 'center'
                                    size_hint_y: .3
                                
                            BoxLayout:
                                orientation: 'vertical'
                                size_hint_y: .5
                                MDRaisedButton:
                                    text: 'Open Discord'
                                    halign: 'center'
                                    pos_hint: {"center_x": .5, "center_y": .5}
                                    on_release: app.webbrowser('https://discord.gg/yJKBCd7')
                            BoxLayout:
                                orientation: 'vertical'
                                size_hint_y: .5

                    Screen:
                        name: 'console'
                        MDBoxLayout:
                            orientation: 'vertical'
                            padding: 0,10,0,0
                            MDLabel:
                                text: 'PS2 Console Setup' 
                                font_style: 'H3'
                                halign: 'center'
                                size_hint_y: .25
                            Image:
                                source: 'refs\ps2.jpg'#app.image_load()
                                #size: self.texture_size
                            MDBoxLayout:
                                orientation:'vertical'
                                padding: 15
                                size_hint_y: .2
                                MDRaisedButton:
                                    text: 'Begin'
                                    on_release: 
                                        screen_manager.transition.direction = 'left'
                                        app.next_button('console1')#screen_manager.current = 'console1'
                                    pos_hint: {"center_x": .5, 'center_y': .5}
                    Screen:
                        name: 'console1'
                        MDBoxLayout:
                            orientation: 'vertical'
                            padding: 0,10,0,0
                            Image:
                                source: 'refs\XLINKdiagram.jpg'#app.image_load()
                                #size: self.texture_size
                            MDLabel:
                                text: 'You should be connected via ethernet for the best connection quality'
                                # font_style: 'H6'
                                halign: 'center'
                                size_hint_y: .1
                            BoxLayout:
                                orientation: 'horizontal'
                                size_hint_y: .2
                                MDBoxLayout:
                                    orientation:'vertical'
                                    padding: 15
                                    MDRaisedButton:
                                        text: 'Back'
                                        on_release: 
                                            screen_manager.transition.direction = 'right'
                                            app.back_button('console')#screen_manager.current = 'console'
                                        pos_hint: {"center_x": .5, "center_y": .5}
                                MDBoxLayout:
                                    orientation:'vertical'
                                    padding: 15
                                    MDProgressBar:
                                        value: 10
                                MDBoxLayout:
                                    orientation:'vertical'
                                    padding: 15
                                    MDRaisedButton:
                                        text: 'Next'
                                        on_release: 
                                            screen_manager.transition.direction = 'left'
                                            app.back_button('console2')#screen_manager.current = 'console'
                                        pos_hint: {"center_x": .5, 'center_y': .5}
                    Screen:
                        name: 'console2'
                        MDBoxLayout:
                            orientation: 'vertical'
                            padding: 0,10,0,0
                            Image:
                                source: 'refs\PCSXmac.jpg'#app.image_load()
