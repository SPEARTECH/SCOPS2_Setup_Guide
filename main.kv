<Share>:
    copy_layout: copy_layout
    orientation: 'vertical'
    BoxLayout:
        orientation: 'horizontal'
        id: copy_layout
        MDLabel:
            text: 'https://discord.gg/3EQRdmY'
        MDIcon:
            icon: 'clipboard-outline'
            size_hint_x: .1
<Pforward>:
    ipitem: ipitem
    orientation: 'vertical'
    spacing: '12dp'
    size_hint_y: None
    height: '120dp'
    MDList:
        OneLineListItem:
            id: ipitem 
            text: 'Get my IP'
            on_release: root.get_ip()
        OneLineListItem:
            text: 'How to Port Forward on your Router'
            on_release: root.launch_pf()
    
