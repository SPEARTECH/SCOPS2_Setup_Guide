    #This is referencing a modded widget
    #MDDesktopNavigationDrawer:
    MDNavigationDrawer:
        id: nav_drawer
        status: 'closed'
        ScrollView:
            do_scroll_y: True
            MDList:
                OneLineListItem:
                    text: 'Setup Guides'
                    font_style: 'Overline'
                OneLineIconListItem:
                    text: 'Console'
                    on_release: 
                        screen_manager.current = 'console'
                        screen_manager.transition.direction = 'left'
                OneLineIconListItem:
                    text: 'Emulator'
                    on_release: 
                        screen_manager.current = 'emulator'
                        screen_manager.transition.direction = 'left'
                OneLineIconListItem:
                    text: 'XLink Kai'
                    on_release: 
                        screen_manager.current = 'xlink'
                        screen_manager.transition.direction = 'left'
                # OneLineIconListItem:
                #     text: 'Troubleshooting'
                #     on_release: 
                #         screen_manager.current = 'troubleshooting'
                #         screen_manager.transition.direction = 'left'
                OneLineListItem:
                    text: 'Social'
                    font_style: 'Overline'
                OneLineIconListItem:
                    text: 'Facebook'
                    on_release: app.webbrowser('https://www.facebook.com/splintercellonlineps2')
                OneLineIconListItem:
                    text: 'YouTube'
                    on_release: app.webbrowser('https://www.youtube.com/channel/UCWe9O6G-B5quHQR_0f7vddQ')
                OneLineIconListItem:
                    text: 'Website'
                    on_release: app.webbrowser('http://www.splintercellonline.net/')
                OneLineIconListItem:
                    text: 'Discord'
                    on_release: app.webbrowser('https://discord.gg/yJKBCd7')


